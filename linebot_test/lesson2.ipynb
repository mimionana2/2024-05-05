    "genai.configure(api_key=os.environ['Gemini_API_KEY'])\n",
    "model=genai.GenerativeModel('gemini-1.5-flash')\n",
    "response=model.generate_content(\"如何學習演算法?\")\n",
    "print(response.text)"
   ]
  }
